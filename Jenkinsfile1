pipeline {
    agent any
    environment
    {
    paramNonuNoni=""
    }
    stages {
        stage('Master Branch Deploy Code') {
            when {
                branch 'master'
            }
            steps {
            echo "${params.pipeline2}"
                bat """
                echo "Building Artifact from Master branch"
                """

                bat """
                echo "Deploying Code from Master branch"
                """
            }
        }
   
        stage('Start') {
        
            steps {
            script{
            env.paramNonuNoni="${params.pipeline2}"
            println(env.paramNonuNoni)
                bat """
                echo %paramNonuNoni%
                """
                }
            }
        }

        
         stage('Build') {
           steps {
               bat """
               echo "Building pipeline"
               """
           }
         }

        stage('End') {
            steps {
                bat """
                echo "End of the pipeline"
                """
            }
        }
    }
}
