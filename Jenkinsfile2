pipeline {
    agent any
    stages {
        stage('Master Branch Deploy Code') {
            when {
                branch 'master'
            }
            steps {
                bat """
                echo "Building Artifact from Master branch"
                """

                bat """
                echo "Deploying Code from Master branch"
                """
            }
        }
stages 
    {
        stage('Start') {
            steps {
                bat'ls'
            }
        }

        stage ('Invoke_Pipeline-2') {
            steps {
                build job: 'Pipeline-1', parameters: [
                string(name: 'param1', value: "value1")
                ]
            }
        }

        stage('End') {
            steps {
                bat 'ls'

        }
        }
    }
}
}
